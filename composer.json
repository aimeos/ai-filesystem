{
	"name": "aimeos/ai-filesystem",
	"description": "Aimeos file system layer extension",
	"keywords": ["aimeos", "extension", "file system", "flysystem"],
	"type": "aimeos-extension",
	"license": "LGPL-3.0",
	"support": {
		"source": "https://github.com/Aimeos/ai-container",
		"issues": "https://github.com/Aimeos/ai-container/issues",
		"forum": "https://aimeos.org/help",
		"wiki": "https://aimeos.org/docs"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/aimeos/php-coveralls.git"
		}
	],
	"prefer-stable": true,
	"minimum-stability": "dev",
	"require": {
		"php": ">=5.3.3",
		"league/flysystem": "~1.0",
		"composer/installers": "~1.0"
	},
	"require-dev": {
		"satooshi/php-coveralls": "dev-master"
	},
	"autoload": {
		"psr-4": {
			"Aimeos\\MW\\": "lib/mwlib/src/MW/"
		},
		"classmap": [
			"lib/custom/src"
		]
	}
}
