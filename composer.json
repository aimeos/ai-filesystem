{
	"name": "aimeos/ai-filesystem",
	"description": "Aimeos file system layer extension",
	"keywords": ["aimeos", "extension", "file system", "flysystem"],
	"homepage": "https://aimeos.org/",
	"type": "aimeos-extension",
	"license": "LGPL-3.0-or-later",
	"support": {
		"source": "https://github.com/aimeos/ai-filesystem",
		"issues": "https://github.com/aimeos/ai-filesystem/issues",
		"forum": "https://aimeos.org/help",
		"wiki": "https://aimeos.org/docs"
	},
	"prefer-stable": true,
	"minimum-stability": "dev",
	"require": {
		"php": "~7.1||~8.0",
		"league/flysystem": "~1.0",
		"aimeos/aimeos-core": "2021.10.*"
	},
	"require-dev": {
		"phpunit/phpunit": "~7.0||~8.0||~9.0"
	},
	"autoload": {
		"psr-4": {
			"Aimeos\\MW\\": "lib/mwlib/src/MW/"
		},
		"classmap": [
			"lib/custom/src"
		]
	}
}
